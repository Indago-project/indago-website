<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Indago\GeneralBundle\Entity\Categorie"
	repository-class="Indago\GeneralBundle\Entity\CategorieRepository" table="Categorie">
    <id name="idCategorie" type="bigint" column="id_categorie">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="libelleCategorie" type="string" column="libelle_categorie" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
