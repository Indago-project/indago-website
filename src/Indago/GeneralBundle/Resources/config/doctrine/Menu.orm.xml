<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Indago\GeneralBundle\Entity\Menu" 
      repository-class="Indago\GeneralBundle\Entity\MenuRepository" table="menu">
    <id name="menuId" type="bigint" column="menu_id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nomMenu" type="string" column="nom_menu" length="128" nullable="true"/>
    <field name="Route" type="string" column="route" length="200" nullable="true"/>
  </entity>
</doctrine-mapping>
